        function isSameProduct(product1,product2){
          
   if(product1.price == product2.price && 
     product1.name == product2.name )
      {
          return true;
      }else{
          return false;
      }
      
      };
          
        
        const product1 = {
            name  : 'sock',
            price : 1095
             
        };
        
        const product2 = {
            name  : 'sock',
            price : 1095
             
       };
        
        console.log(isSameProduct(product1 , product2))
        
