        function comparePrice(prodect1,prodect2){
          
   if(prodect1.price < prodect2.price)
      {
          return prodect1;
      }else{
          return prodect2;
      }
      
      };
          
        
        const prodect1 = {
            name  : 'ball',
            price : 1095
             
        };
        
        const prodect2 = {
            name  : 'socke',
            price : 2095
             
       };
        
        console.log(comparePrice(prodect1 , prodect2))
              
         <!-- end 8 d -->   
         
